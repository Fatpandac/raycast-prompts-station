{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "prompts-station",
  "title": "Prompts Station",
  "description": "Save prompts localy and run quickly",
  "icon": "command-icon.png",
  "author": "Fatpandac",
  "categories": [
    "Applications",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Run Prompt",
      "description": "Run your local prompt",
      "mode": "view"
    },
    {
      "name": "createPrompts",
      "title": "Create Prompt",
      "description": "Create your local prompt",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.1",
    "dayjs": "^1.11.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}